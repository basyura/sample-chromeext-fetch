{
  "name": "My Extension",
  "description": "My Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting", "http://localhost:1323/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches":[
        "http://basyura.org/"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background" : {
    "service_worker" : "background.js"
  }
}


